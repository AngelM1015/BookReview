<%= simple_form_for @book, :html => { :multipart => true } do |f|%>
  <%= f.file_field :book_img %>
  <%= f.input :title, label: "Book Title"%>
  <%= f.input :description %>
  <%= f.input :author %>
  <%= f.input :author %>
  <%= f.button :submit %>

  <%= form_for @category.each do |f|%>
  <%= f.input :name%>
  <%= f.button :submit %>
  <% end %>
<% end %>
